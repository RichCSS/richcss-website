.menu-list{
	margin: 0;
	padding: 0;
}
	.menu-list ul{
		margin: 0;
		padding: 0;
	}
		.menu-list ul li{
			display: inline-block;
			margin-right: 44px;
		}
		.menu-list ul li:last-child{
			margin-right: 0;
		}
			.menu-list ul li a{
				font-size: 16px;
				line-height: 17px;
			}
			.menu-list ul li a:hover{
				color: #d0021b;
			}
/*Home page menu ends*/
.inner-page .menu-list{
	margin-top: 20px;
}
	.menu-list ul li.active a{
		position: relative;	
	}
