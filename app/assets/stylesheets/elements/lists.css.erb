/*Inner page menu ends*/
.be-af-list>ul>li{
	margin-bottom: 10px;
}
	.be-af-list>ul>li>ul{
		margin-top: 10px;
	}
		.be-af-list>ul>li>ul>li{
			margin-bottom: 10px;
		}
			.be-af-list>ul>li>ul>li>ul{
				margin-top: 12px;
			}
				.be-af-list>ul>li>ul>li>ul>li{
					margin-bottom: 10px;
				}
					.be-af-list>ul>li>ul>li>ul>li>ul{
						margin-top: 12px;
					}
						.be-af-list>ul>li>ul>li>ul>li>ul>li{
							margin-bottom: 10px;
						}
.be-af-list-first>li>span:before, .be-af-list-second>li>span:before{
	content: '';
	height: 20px;
	background-image: url(<%= asset_path('folder.svg') %>);
	-webkit-background-size: cover;
	vertical-align: bottom;
	background-size: cover;
	margin-right: 12px;
	width: 20px;
	display: inline-block;

}
	.list-second{
		position: relative;
	}
	.list-second:before{
		content: '';
		display: inline-block;
		position: absolute;
		width: 1px;
		height: 80px;
		background-color: #4a90e2;
		opacity: .5;
		left: -15px;
	}
		.be-af-list-third>li>span:before{
			content: '';
			height: 20px;
			background-image: url(<%= asset_path('sheet.svg') %>);
			-webkit-background-size: cover;
			vertical-align: bottom;
			background-size: cover;
			margin-right: 7px;
			width: 18px;
			display: inline-block;
		}
		.be-af-list-second{
			margin-left: 20px;
		}
			.be-af-list-third{
				margin-left: 30px;
			}
				.be-af-list ul li span{
					color: #4a90e2;
					display: block;
					font-family: AvenirLight;
					font-size: 15px;
				}
/*Before After Listing Ends*/
.left-nav{
	margin: 0;
	font-family: HelveticaBold;
	padding: 0;
}
	.left-nav>ul{
		margin: 0;
		padding: 0;
	}
		.left-nav>ul>li{
			margin-bottom: 5px;
		}
			.left-nav>ul>li>ul>li{
				margin-top: 5px;
			}
			.left-nav>ul>li>ul>li:before{
				content: '\-';
				display: inline-block;
				margin-right: 2px;
			}
				.left-nav>ul>li>ul>li>a{
					color: #494848;
					font-size: 14px;
					font-family: HelveticaBold;
				}
				.left-nav>ul>li>ul>li>a.active{
					color: #e37e52;
				}
			.left-nav>ul>li>a{
				color: #494848;
				font-size: 14px;
				font-family: HelveticaBold;
			}
			.left-nav>ul>li>a.active{
				color: #d0021b;
			}
/*Inner page side menu ends*/
